<?xml version="1.0" encoding="UTF-8"?>

<profile name="testsync-ovi" type="sync" >

    <key name="enabled" value="true" />

    <key name="Remote database" value="https://sync.ovi.com:443/services/syncml" />

    <key name="Username" value="your_username" />
    <key name="Password" value="your_password" />

    <key name="http_proxy_host" value="" />
    <key name="http_proxy_port" value="" />

    <key name="destinationtype" value="online" />

    <profile name="syncml" type="client" >
       <key name="WantB64" value="false" />
       <key name="use_wbxml" value="false" />
       <key name="Sync Transport" value="HTTP" />
    </profile>

    <profile name="hcalendar" type="storage" >
        <key name="enabled" value="false" />
	<key name="Notebook Name" value="Something" />
        <key name="Target URI" value="./EventTask/Tasks" />
        <key name="Calendar Format" value="vcalendar" />
    </profile>

    <profile name="hcontacts" type="storage" >
	<key name="enabled" value="true" />
    </profile>
    
</profile>
